<div class="space-y-6" id="education-list">
    <% educations.forEach(function(edu) { %>
    <div class="border border-gray-200 rounded-lg p-6">
      <div class="flex justify-between">
        <div>
          <h4 class="font-semibold text-gray-900"><%= edu.degree %> In <%= edu.field_of_study %></h4>
          <p class="text-indigo-600 text-sm">From <%= edu.institution %></p>
          <p class="text-sm text-gray-600"> CGPA <%= edu.grade %> </p>
        </div>
        <div class="flex gap-2">
          <button class="p-1.5 hover:bg-gray-100 rounded-lg text-gray-500" title="Edit">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z" />
            </svg>
          </button>
          <button class="p-1.5 hover:bg-gray-100 rounded-lg text-gray-500" title="Delete">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16" />
            </svg>
          </button>
        </div>
      </div>
      <p class="mt-3 text-gray-600"><%= edu.description %></p>
    </div>
    <% }); %>
  </div>